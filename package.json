{
  "name": "lms-backend",
  "version": "0.7.8",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "pnpm run nestia:swagger && NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run nestia:sdk && NODE_ENV=test jest --config test/e2e/jest-e2e.json",
    "test:e2e:watch": "NODE_ENV=test jest --config test/e2e/jest-e2e.json --watch",
    "prepare": "ts-patch install && typia patch",
    "nestia:build:sdk:prod": "NODE_ENV=production nestia sdk",
    "nestia:swagger": "NODE_ENV=development nestia swagger",
    "nestia:sdk": "NODE_ENV=development nestia sdk",
    "nestia:build": "pnpm run nestia:swagger && pnpm run nestia:sdk",
    "db:push:dev": "dotenv -e .env.development -- drizzle-kit push",
    "db:migrate:gen:dev": "dotenv -e .env.development -- drizzle-kit generate",
    "db:seed:dev": "pnpm run nestia:build && pnpm run db:push:dev && dotenv -e .env.development -- ts-node drizzle.seed.ts",
    "db:snapshot:dev": "NODE_ENV=development ./db-snapshot.sh",
    "db:snapshot:restore:dev": "NODE_ENV=development ./db-restore.sh",
    "db:snapshot:restore:stg": "NODE_ENV=stg ./db-restore.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.621.0",
    "@aws-sdk/cloudfront-signer": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.621.0",
    "@nestia/core": "^3.12.1",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.12.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@node-rs/argon2": "^1.8.3",
    "@testcontainers/postgresql": "^10.11.0",
    "axios": "^1.7.2",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.12",
    "drizzle-orm": "^0.33.0",
    "jose": "^5.6.3",
    "libphonenumber-js": "^1.11.9",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "solapi": "^5.3.1",
    "testcontainers": "^10.10.4",
    "typia": "^6.9.0"
  },
  "devDependencies": {
    "@nestia/benchmark": "^0.2.3",
    "@nestia/sdk": "^3.12.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@snaplet/snapshot": "^0.93.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^6.0.1",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
